.request-progress.mt-2.mt-sm-0
  .progress
    .progress-bar.bg-success{ role: 'progressbar', style: "width:#{li.percent_progress}%;", aria: { valuenow: li.lab_count, valuemin: 0, valuemax: li.number_of_specimens_requested } }
    %span.progress-text
      = t('requests.table.progress', received: li.lab_count, total: li.number_of_specimens_requested)
