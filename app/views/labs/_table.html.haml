.table-overflow
  %table.table#labsTable
    %thead
      %tr
        %th.col-2
          = t(:labs)[:fields][:mrn]
        %th.col-2
          = t(:labs)[:fields][:samples_available]
        %th.col-3
          = t(:labs)[:fields][:specimen_source]
        %th.col-3
          = t(:labs)[:fields][:protocols]
        %th.col-2.text-center
          = t(:labs)[:fields][:actions]
    %tbody
      - if lab_groups.any?
        - lab_groups.each do |grouping, labs|
          %tr
            %td
              = grouping[:patient].mrn
            %td
              = labs.count
            %td
              = grouping[:specimen_source]
            %td
              = protocols_preview(grouping[:patient].protocols)
            %td.text-center
              = release_lab_button(grouping[:patient])
      - else
        %tr
          %td.text-center{ colspan: 5 }
            = "No records to show."
